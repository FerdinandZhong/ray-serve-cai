# CAI / CML Configuration for Testing
# Copy this file to .env and fill in your values
# Then source it: source .env

# CML Instance URL
export CML_HOST="https://ml.example.cloudera.site"

# CML API Key (get from your CML profile settings)
export CML_API_KEY="your-api-key-here"

# CML Project ID (get from project URL or API)
export CML_PROJECT_ID="your-project-id"

# Optional: Conda environment name (if using conda)
# export CONDA_ENV_NAME="ray-serve-env"
# export USE_CONDA="true"

# Optional: Add caikit library to Python path
# export PYTHONPATH="/path/to/caikit:$PYTHONPATH"
